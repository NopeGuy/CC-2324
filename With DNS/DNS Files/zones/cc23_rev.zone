;
; BIND reverse data file for local loopback interface
;
$TTL    604800
@    IN    SOA    cc23. root.cc23. (
                  4        ; Serial
             604800        ; Refresh
              86400        ; Retry
            2419200        ; Expire
             604800 )    ; Negative Cache TTL
;
@    IN    NS    Servidor1.cc23.
1.4.4.10.in-addr.arpa.    IN    PTR    Servidor1.cc23.
2.4.4.10.in-addr.arpa.    IN    PTR    Servidor2.cc23.
1.1.1.10.in-addr.arpa.    IN    PTR    Portatil1.cc23.
2.1.1.10.in-addr.arpa.    IN    PTR    Portatil2.cc23.
1.2.2.10.in-addr.arpa.    IN    PTR    PC1.cc23.
2.2.2.10.in-addr.arpa.    IN    PTR    PC2.cc23.
1.3.3.10.in-addr.arpa.    IN    PTR    Roma.cc23.
2.3.3.10.in-addr.arpa.    IN    PTR    Paris.cc23.
